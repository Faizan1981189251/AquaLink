{
  "expo": {
    "name": "AquaLink",
    "slug": "aqualink-water-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aqualink",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCUtljQ7ydcbT33YJwMK22c678bz9pmt4g"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2563EB"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCUtljQ7ydcbT33YJwMK22c678bz9pmt4g"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-location",
      "expo-camera",
      "expo-barcode-scanner",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow AquaLink to use your location to find nearby water suppliers and track deliveries."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AquaLink to use your camera to scan product barcodes for quick ordering."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}